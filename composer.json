{
  "name": "strictlyphp/domantra",
  "description": "A principled PHP toolkit for building DDD + CQRS services with clarity and structure.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ken Lalobo",
      "email": "you@example.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "StrictlyPHP\\Domantra\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StrictlyPHP\\Domantra\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
