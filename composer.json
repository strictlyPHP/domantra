{
  "name": "strictlyphp/domantra",
  "version": "0.0.4",
  "description": "A principled PHP toolkit for building DDD + CQRS services with clarity and structure.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ken Lalobo",
      "email": "lalobo@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "predis/predis": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "StrictlyPHP\\Domantra\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StrictlyPHP\\Tests\\Domantra\\": "tests/"
    }
  },
  "require-dev": {
    "symplify/easy-coding-standard": "^11.1 || ^12.0",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan-phpunit": "^1.1",
    "php-coveralls/php-coveralls": "^2.7"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
